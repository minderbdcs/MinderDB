DECLARE VARIABLE WK_PICK_LOCATION VARCHAR(10);
DECLARE VARIABLE WK_SALE_PRICE NUMERIC(9,3);
DECLARE VARIABLE WK_DISCOUNT DOUBLE PRECISION;
