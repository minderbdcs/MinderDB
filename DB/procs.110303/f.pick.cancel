DECLARE VARIABLE WK_CHECKIN_START TIMESTAMP;
DECLARE VARIABLE WK_CHECKIN_FINISH TIMESTAMP;
DECLARE VARIABLE WK_CHECKIN_USER_ID VARCHAR(10);
DECLARE VARIABLE WK_PARTIAL_PICK_ALLOWED CHAR(1);
