ALTER PROCEDURE GET_GLOBAL_CONDITION (OTHER_NO INTEGER,
RESULT CHAR(1) )
RETURNS (DESCRIPTION VARCHAR(30) )
AS 

BEGIN
  SELECT FIRST 1 DESCRIPTION FROM GLOBAL_CONDITIONS
  WHERE OTHER_NO = :OTHER_NO
  AND RESULT = :RESULT
  INTO :DESCRIPTION;
  SUSPEND;

END ^


