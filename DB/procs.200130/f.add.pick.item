CREATE OR ALTER PROCEDURE ADD_PICK_ITEM_TEMP (H_PICK_ORDER PICK_ORDER,
H_PARTIAL_PICK_ALLOWED VARCHAR(1) ,
H_CREATE_DATE TIMESTAMP,
H_PICK_DUE_DATE TIMESTAMP,
H_SHIP_VIA SHIP_VIA ,
H_OTHER1 VARCHAR(50) ,
H_OTHER2 VARCHAR(50) ,
H_OTHER3 VARCHAR(50) ,
H_OTHER4 VARCHAR(50) ,
H_OTHER5 VARCHAR(50) ,
H_OTHER6 VARCHAR(50) ,
H_OTHER7 VARCHAR(50) ,
H_OTHER8 VARCHAR(50) ,
H_OTHER9 VARCHAR(255) ,
H_PERSON_ID PERSON,
H_CONTACT_NAME CONTACT_NAME ,
H_PICK_PRIORITY SMALLINT,
H_SPECIAL_INSTRUCTIONS1 SPECIAL_INSTRUCTIONS1 ,
H_SPECIAL_INSTRUCTIONS2 SPECIAL_INSTRUCTIONS2 ,
H_PICK_STATUS STATUS ,
P_FIRST_NAME FIRST_NAME ,
P_LAST_NAME LAST_NAME ,
P_ADDRESS_LINE1 ADDRESS_LINES ,
P_ADDRESS_LINE2 ADDRESS_LINE ,
P_ADDRESS_LINE3 ADDRESS_LINE ,
P_ADDRESS_LINE4 ADDRESS_LINE ,
P_CITY CITY,
P_STATE STATE ,
P_POST_CODE POST_CODE ,
P_COUNTRY COUNTRY ,
P_AUST_POST_4STATE_ID VARCHAR(255) ,
L_PICK_ORDER_LINE_NO VARCHAR(4) ,
L_PROD_ID PRODUCT,
L_PICK_ORDER_QTY QTY)
AS 
BEGIN 
   INSERT INTO PICK_ORDER_ITEM_TEMP( H_PICK_ORDER, H_PARTIAL_PICK_ALLOWED, H_CREATE_DATE, H_PICK_DUE_DATE,
H_SHIP_VIA, H_OTHER1, H_OTHER2, H_OTHER3,
H_OTHER4, H_OTHER5, H_OTHER6, H_OTHER7, H_OTHER8,
H_OTHER9, H_PERSON_ID, H_CONTACT_NAME, H_PICK_PRIORITY, H_SPECIAL_INSTRUCTIONS1,
H_SPECIAL_INSTRUCTIONS2, H_PICK_STATUS, P_FIRST_NAME, P_LAST_NAME, P_ADDRESS_LINE1,
P_ADDRESS_LINE2, P_ADDRESS_LINE3, P_ADDRESS_LINE4, P_CITY, P_STATE,
P_POST_CODE, P_COUNTRY, P_AUST_POST_4STATE_ID, L_PICK_ORDER_LINE_NO, L_PROD_ID, L_PICK_ORDER_QTY)
      VALUES( :H_PICK_ORDER, :H_PARTIAL_PICK_ALLOWED, :H_CREATE_DATE, :H_PICK_DUE_DATE,
:H_SHIP_VIA, :H_OTHER1, :H_OTHER2, :H_OTHER3,
:H_OTHER4, :H_OTHER5, :H_OTHER6, :H_OTHER7, :H_OTHER8,
:H_OTHER9, :H_PERSON_ID, :H_CONTACT_NAME, :H_PICK_PRIORITY, :H_SPECIAL_INSTRUCTIONS1,
:H_SPECIAL_INSTRUCTIONS2, :H_PICK_STATUS, :P_FIRST_NAME, :P_LAST_NAME, :P_ADDRESS_LINE1,
:P_ADDRESS_LINE2, :P_ADDRESS_LINE3, :P_ADDRESS_LINE4, :P_CITY, :P_STATE,
:P_POST_CODE, :P_COUNTRY, :P_AUST_POST_4STATE_ID, :L_PICK_ORDER_LINE_NO, :L_PROD_ID, :L_PICK_ORDER_QTY);

   SUSPEND; 
END ^


SET TERM ; ^
COMMIT WORK;
SET AUTODDL ON;
