COMMIT WORK;
SET AUTODDL OFF;
SET TERM ^ ;


CREATE OR ALTER TRIGGER AU_VALID_RESPONSES_RESPONSE FOR VALID_RESPONSES 
ACTIVE BEFORE UPDATE POSITION 0 
AS
BEGIN
  IF (NEW.VALID_RESPONSE IS NULL) THEN
      NEW.VALID_RESPONSE = '';
  ELSE
  BEGIN
/*    NEW.VALID_RESPONSE = ALLTRIM(NEW.VALID_RESPONSE); */
      NEW.VALID_RESPONSE = TRIM(NEW.VALID_RESPONSE);
  END
END ^
 

SET TERM ; ^
COMMIT WORK;
SET AUTODDL ON;
