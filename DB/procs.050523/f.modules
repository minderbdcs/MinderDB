CREATE DOMAIN VALUE_FT AS CHAR(1)
         DEFAULT 'T'
         CHECK (VALUE IN ('T','F')) ;

CREATE TABLE MODULE (
        MODULE_ID CHAR(2) NOT NULL,
        MODULE_SUB_TYPE CHAR(2) NOT NULL,
        DESCRIPTION DESCRIPTION,
        LICENSE_NO CODE ,
        ENABLED VALUE_TF ,
PRIMARY KEY (MODULE_ID, MODULE_SUB_TYPE));

ALTER TABLE SYS_MOVES ADD MOVE_ALLOWED VALUE_FT;
UPDATE SYS_MOVES SET MOVE_ALLOWED='T';


