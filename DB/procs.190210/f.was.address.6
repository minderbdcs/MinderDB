        END
        WK_PD_LAST_LINE = WK_PD_LAST_LINE + 1;
     WK_LOG_RESULT = FILE_WRITELN(WK_LOG_FILENAME, :WK_PD_LAST_LINE);
     END

    WK_LOG_RESULT = FILE_WRITELN(WK_LOG_FILENAME, 'end of of ADDRESS Filter');
    SUSPEND;
END ^


