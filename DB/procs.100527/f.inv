CREATE DOMAIN INVOICE_ID INTEGER;
CREATE DOMAIN INVOICE_NO VARCHAR(10);

ALTER TABLE PICK_DESPATCH ADD INVOICE_ID INVOICE_ID;

CREATE INDEX PICK_DESPATCH_INVOICE_IDX ON PICK_DESPATCH(INVOICE_ID);

CREATE TABLE PICK_INVOICE (
	INVOICE_ID INVOICE_ID NOT NULL,
	INVOICE_NO INVOICE_NO ,
        CREATE_DATE CREATE_DATE,
        INVOICE_STATUS STATUS,
        LAST_UPDATE_DATE LAST_UPDATE_DATE,
PRIMARY KEY (INVOICE_ID));

