COMMIT WORK;
SET AUTODDL OFF;
SET TERM ^ ;


CREATE OR ALTER PROCEDURE UPDATE_TRAN4 (RECORD_ID INTEGER,
COMPLETE CHAR(1) ,
ERROR_TEXT VARCHAR(255) )
AS 
 
BEGIN
    /* Update transactions4 table */
    UPDATE TRANSACTIONS4
    SET COMPLETE = :COMPLETE, ERROR_TEXT = :ERROR_TEXT,
    MIRROR_DATA = TRN_DATE
    WHERE RECORD_ID = :RECORD_ID;
 
    SUSPEND;
END ^


SET TERM ; ^
COMMIT WORK;
SET AUTODDL ON;
