COMMIT WORK;
SET AUTODDL OFF;
SET TERM ^ ;


CREATE OR ALTER TRIGGER MOD_PACK_MANIFEST_MIRROR FOR PACK_MANIFEST
ACTIVE BEFORE UPDATE POSITION 4 
AS
BEGIN
   IF ( OLD.MIRROR_DATA IS NOT NULL) THEN
   BEGIN
      /* currently mirror data is populated */
      NEW.MIRROR_DATA = NULL;
   END

END ^


SET TERM ; ^
COMMIT WORK;
SET AUTODDL ON;
