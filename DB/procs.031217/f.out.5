/*
CREATE PROCEDURE UPDATE_EMP_SETUP
*/
ALTER  PROCEDURE UPDATE_EMP_SETUP
AS
DECLARE VARIABLE WK_EMP_NO VARCHAR(10);
DECLARE VARIABLE WK_FIELD  VARCHAR(255);
BEGIN
FOR SELECT EMPLOYEE_ID FROM EMPLOYEE
WHERE EMPLOYEE.ADDRESS_LINE1 IS NULL
INTO :WK_EMP_NO
DO
BEGIN
   SELECT ADDRESS_LINE1 FROM PERSON WHERE PERSON_ID = :WK_EMP_NO INTO :WK_FIELD;
   UPDATE EMPLOYEE SET ADDRESS_LINE1  =  :WK_FIELD
   WHERE EMPLOYEE_ID = :WK_EMP_NO;
END
END^
COMMIT^
EXECUTE PROCEDURE UPDATE_EMP_SETUP^
COMMIT^
