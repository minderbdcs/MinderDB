COMMIT WORK;
SET AUTODDL OFF;
SET TERM ^ ;



CREATE OR ALTER TRIGGER PICK_DESPATCH_DETAIL_ID FOR PICK_DESPATCH 
ACTIVE BEFORE INSERT POSITION 0 
AS
BEGIN
  IF (NEW.DESPATCH_ID IS NULL) THEN
      NEW.DESPATCH_ID = GEN_ID(DESPATCH_ID_GEN, 1);
  IF (NEW.DESPATCH_ID = 0) THEN
      NEW.DESPATCH_ID = GEN_ID(DESPATCH_ID_GEN, 1);
END ^
 

SET TERM ; ^
COMMIT WORK;
SET AUTODDL ON;
