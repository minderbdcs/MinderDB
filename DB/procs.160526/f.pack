CREATE OR ALTER  TRIGGER PACK_DETAIL_ID FOR PACK_ID 
ACTIVE BEFORE INSERT POSITION 0 
AS
BEGIN
  IF (NEW.PACK_ID IS NULL) THEN
      NEW.PACK_ID = GEN_ID(DESPATCH_PACK_ID_GEN, 1);
  IF (NEW.PACK_ID = 0) THEN
      NEW.PACK_ID = GEN_ID(DESPATCH_PACK_ID_GEN, 1);
END ^
 

SET TERM ; ^
COMMIT WORK;
SET AUTODDL ON;
