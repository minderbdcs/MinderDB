COMMIT WORK;
SET AUTODDL OFF;
SET TERM ^ ;
 
DROP   TRIGGER UPDATE_SSN_TIME ^
COMMIT^

CREATE TRIGGER UPDATE_SSN_TIME FOR SSN 
ACTIVE BEFORE UPDATE POSITION 20 
AS
BEGIN
   NEW.LAST_UPDATE_DATE = 'NOW';
   IF (NEW.CURRENT_QTY < 0) THEN
   BEGIN
      NEW.CURRENT_QTY = 0;
   END
END ^


SET TERM ; ^
COMMIT WORK;
SET AUTODDL ON;
