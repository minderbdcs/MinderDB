alter table pick_despatch add pickd_manifest_id integer;


CREATE TABLE PICK_MANIFEST (MANIFEST_ID DESPATCH_ID NOT NULL,
        PICKM_DATE DESPATCH_FINISH,
        USER_ID USER_ID,
        DEVICE_ID DEVICE_ID,
        CREATE_DATE CREATE_DATE,
        LAST_UPDATE_DATE LAST_UPDATE_DATE,
        PICKM_STATUS STATUS,
PRIMARY KEY (MANIFEST_ID));

