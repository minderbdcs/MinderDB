COMMIT WORK;
SET AUTODDL OFF;
SET TERM ^ ;
 
DROP TRIGGER ADD_SSN_TIME ^
COMMIT^

CREATE TRIGGER ADD_SSN_TIME FOR SSN 
ACTIVE BEFORE INSERT POSITION 20 
AS
BEGIN
   NEW.LAST_UPDATE_DATE = 'NOW';
   IF (NEW.CREATE_DATE IS NULL) THEN
   BEGIN
      NEW.CREATE_DATE = 'NOW';
   END
END ^


SET TERM ; ^
COMMIT WORK;
SET AUTODDL ON;
