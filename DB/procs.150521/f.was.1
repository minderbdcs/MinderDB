SELECT SSN.SSN_ID, SSN.WH_ID, SSN.LOCN_ID, SSN.HOME_LOCN_ID
             FROM SSN
             JOIN COST_CENTRE ON SUBSTR(SSN.LOCN_ID,1,2) = COST_CENTRE.CODE
             WHERE SSN.WH_ID = 'XB'
               AND SUBSTR(SSN.LOCN_ID,3,8) = '000000'
               AND (SSN.HOME_LOCN_ID <> SSN.LOCN_ID
               OR SSN.HOME_LOCN_ID IS NULL)
;
