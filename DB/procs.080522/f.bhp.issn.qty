
CREATE TABLE ISSN_QTY (SSN_ID SSN NOT NULL,
        CURRENT_QTY QTY ,
        PREV_QTY QTY ,
        CREATE_DATE CREATE_DATE,
        CREATED_BY USER_ID);

INSERT INTO ISSN_QTY(SSN_ID, CURRENT_QTY, PREV_QTY, CREATE_DATE, CREATED_BY) SELECT SSN_ID, CURRENT_QTY, 0 , 'TODAY','BDCS' FROM ISSN WHERE CURRENT_QTY > 0;

