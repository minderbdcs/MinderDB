ALTER PROCEDURE GET_REFERENCE_FIELD (REFERENCE VARCHAR(256) ,
FIELD_NO INTEGER)
RETURNS (WK_FIELD_TEXT VARCHAR(256) )
AS 
 
BEGIN
  
  IF (STRLEN(:REFERENCE) = 0) THEN
  BEGIN     
     EXIT;
  END
  
  WK_FIELD_TEXT = ALLTRIM(PARSE(REFERENCE, '|', FIELD_NO));
  IF (WK_FIELD_TEXT = 'Token to long in parse') THEN
  BEGIN
     WK_FIELD_TEXT = '';
  END
  SUSPEND;
END ^


SET TERM ; ^
COMMIT WORK;
SET AUTODDL ON;
