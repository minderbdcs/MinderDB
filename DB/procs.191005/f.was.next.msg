/*
ALTER PROCEDURE GET_NEXT_MESSAGE RETURNS(MESSAGE_ID VARCHAR(10) )
*/
CREATE GENERATOR MESSAGE_ID_GEN^

CREATE PROCEDURE GET_NEXT_MESSAGE RETURNS(MESSAGE_ID VARCHAR(10) )
AS
BEGIN
     MESSAGE_ID = CAST(GEN_ID(MESSAGE_ID_GEN, 1) AS VARCHAR(10));
     SUSPEND;
END^


