/*
add table sys_equip_device_options
with device id
param code
param value
    SEAD_LAST_UPDATE_DATE TIMESTAMP
    SEAD_LAST_UPDATE_BY USER_ID
*/

CREATE TABLE SYS_EQUIP_DEVICE_OPTIONS (
RECORD_ID RECORD_ID NOT NULL,
SEDO_DEVICE_ID DEVICE_ID NOT NULL,
SEDO_CODE CODE,
SEDO_VALUE CODE,
SEDO_LAST_UPDATE_DATE LAST_UPDATE_DATE,
SEDO_LAST_UPDATED_BY   USER_ID,
PRIMARY KEY (RECORD_ID));

