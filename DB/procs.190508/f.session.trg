
COMMIT WORK;
SET AUTODDL OFF;
SET TERM ^ ;
 

CREATE TRIGGER ADD_SESSION FOR SESSION 
ACTIVE BEFORE INSERT POSITION 10 
AS
BEGIN
   NEW.LAST_UPDATE_DATE = 'NOW';
END ^

CREATE TRIGGER UPD_SESSION FOR SESSION 
ACTIVE BEFORE UPDATE POSITION 10 
AS
BEGIN
   NEW.LAST_UPDATE_DATE = 'NOW';
END ^


SET TERM ; ^
COMMIT WORK;
SET AUTODDL ON;
