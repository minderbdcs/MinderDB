alter table log add create_date timestamp;

COMMIT WORK;
SET AUTODDL OFF;
SET TERM ^ ;
 
DROP   TRIGGER LOG_ID ^
COMMIT^

CREATE TRIGGER LOG_ID FOR LOG 
ACTIVE BEFORE INSERT POSITION 0 
AS
BEGIN
  IF (NEW.SEQ IS NULL) THEN
      NEW.SEQ = GEN_ID(LOG_GEN, 1);
  NEW.CREATE_DATE = 'NOW';
END ^


SET TERM ; ^
COMMIT WORK;
SET AUTODDL ON;
